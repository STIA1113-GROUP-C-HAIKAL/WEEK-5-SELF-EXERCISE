package SelfExercise;

public class SwitchBreak {

	public static void main(String[] args) {
		
		//Data types
		int m = 1;
		switch	(m)
		{
		case 1 :
			System.out.println("m=1");
			break;
		case 2 :
			System.out.println("m=2");
			break;
		case 3 :
			System.out.println("m=3");
			break;
			default :
				System.out.println("default");}
		}

	}
